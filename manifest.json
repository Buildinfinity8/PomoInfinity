{
  "name": "PomoInfinity",
  "description": "PomoInfinity Unlike traditional timers that track one session at a time, It allows you to create a dynamic queue of specific work tasks, manage them individually, and visualize your progress with a clean interface.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
        "nativeMessaging"  
  ],
  
  "icons": {
    "16": "files/images/logo/logo-16.png",
    "48": "files/images/logo/logo-48.png",
    "128": "files/images/logo/logo-128.png"
  },
  "background": {
    "service_worker": "files/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "files/images/logo/logo-16.png",
      "36": "files/images/logo/logo-36.png",
      "48": "files/images/logo/logo-48.png",
      "128": "files/images/logo/logo-128.png"
    }
  }
}